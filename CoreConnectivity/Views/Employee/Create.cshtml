@model Employee




<h1>Add Employee</h1>
    <form method="post" asp-action="Create" asp-controller="Employee">
<div class="container">
    <div class="row">
        <div class="col-mid-6">
            <label>Name</label>
            <input type="text" class="form-control" asp-for="Name" />
        </div>
    </div>
    <div class="row">
        <div class="col-mid-6">
            <label>Age</label>
            <input type="text" class="form-control" asp-for="Age" />
        </div>
    </div>
    <div class="row">
        <div class="col-mid-6">
            <label>Address</label>
            <input type="text" class="form-control" asp-for="Address" />
        </div>
    </div>
    <div class="row">
        <div class="col-mid-6">
            <label>Mobileno</label>
            <input type="text" class="form-control" asp-for="Mobileno" />
        </div>
    </div>
    <div class="row">
        <div class="col-mid-6">
            <label>Salary</label>
            <input type="text" class="form-control" asp-for="Salary" />
        </div>
    </div>
    <div class="row mt-3">
        <div class="col-mid-6">
            <button class="form-control btn btn-success">Register</button>
        </div>
    </div>
    <a asp-action="Index" asp-controller="Employee" class="btn btn-primary mt-3">Back to List</a>
</div>
</form>